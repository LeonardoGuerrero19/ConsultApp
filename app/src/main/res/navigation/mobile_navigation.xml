<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_calendario">

    <fragment
        android:id="@+id/navigation_calendario"
        android:name="com.example.consultapp.ui.calendario.CalendarioFragment"
        android:label="@string/title_calendario"
        tools:layout="@layout/fragment_calendario" />

    <fragment
        android:id="@+id/navigation_horarios"
        android:name="com.example.consultapp.ui.horarios.HorariosFragment"
        android:label="@string/title_horarios"
        tools:layout="@layout/fragment_horarios" />

    <fragment
        android:id="@+id/navigation_expedientes"
        android:name="com.example.consultapp.ui.expedientes.ExpedientesFragment"
        android:label="@string/title_expedientes"
        tools:layout="@layout/fragment_expedientes" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.consultapp.ui.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />

</navigation>